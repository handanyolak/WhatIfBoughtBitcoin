{
  "name": "whatIf",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install; rm -rf .husky/pre-push; husky add .husky/pre-push 'yarn run hook:pre-push'; rm -rf .husky/_/.gitignore",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "analyze": "nuxt build --analyze",
    "test": "jest",
    "format": "eslint '**/{*,''}.{json,js,vue}' --fix; stylelint '**/*.{css,scss,vue}' --fix; prettier '**/{*,''}.{css,scss,json,js,vue}' --write;",
    "format:css": "stylelint '**/*.css' --fix; prettier '**/*.css' --write;",
    "format:scss": "stylelint '**/*.scss' --fix; prettier '**/*.scss' --write;",
    "format:json": "eslint '**/{*,''}.json' --fix; prettier '**/{*,''}.json' --write;",
    "format:js": "eslint '**/{*,''}.js' --fix; prettier '**/{*,''}.js' --write;",
    "format:vue": "eslint '**/*.vue' --fix; stylelint '**/*.vue' --fix; prettier '**/*.vue' --write;",
    "lint": "cspell lint --gitignore '**/{*,''}{.,''}*'; eslint '**/{*,''}.{json,js,vue}'; prettier '**/{*,''}.{css,scss,json,js,vue}' --check; stylelint '**/*.{css,scss,vue}';",
    "hook:pre-push": "cspell lint --gitignore '**/{*,''}{.,''}*' && eslint '**/{*,''}.{json,js,vue}' && prettier '**/{*,''}.{css,scss,json,js,vue}' --check && stylelint '**/*.{css,scss,vue}'"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.31.0",
    "@nuxtjs/recaptcha": "^1.0.4",
    "core-js": "^3.19.3",
    "nuxt": "^2.15.8",
    "vee-validate": "^3.4.14",
    "vue-toastify": "^1.8.1",
    "web3": "^1.7.1",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@mdi/js": "^6.5.95",
    "@nuxtjs/eslint-config": "^9.0.0",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/tailwindcss": "^5.0.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "cspell": "^5.19.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nuxt-purgecss": "^1.0.0",
    "postcss": "^8.4.8",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-prettier": "^2.0.0",
    "vue-jest": "^3.0.7"
  }
}
